{
  "name": "otus-course",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.ts",
    "calc": "ts-node src/_tasks-outside-the-main-project/calc/index.ts",
    "lint-staged": "lint-staged",
    "lint:js": "eslint --ext .js,.ts,.tsx src/",
    "lint:js:fix": "eslint --ext .js,.ts,.tsx --fix src/",
    "pretty": "prettier --write **/{*.ts,*tsx,*.js,*.json}",
    "test": "jest",
    "prepare": "husky install"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-proposal-decorators": "^7.17.9",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@stryker-mutator/core": "^5.6.1",
    "@stryker-mutator/jest-runner": "^5.6.1",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.4",
    "babel-plugin-named-asset-import": "^0.3.8",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "dotenv-webpack": "^7.1.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "path": "^0.12.7",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "stryker": "^0.35.1",
    "style-loader": "^3.3.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/*.{js,ts}": "eslint --fix",
    "*.{js,ts,md,json,yml,html}": "prettier --write"
  },
  "overrides": {
    "node-ipc@>9.2.1 <10": "9.2.1",
    "node-ipc@>10.1.0": "10.1.0",
    "es5-ext@": "0.10.53"
  }
}
